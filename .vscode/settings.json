{
  "editor.bracketPairColorization.independentColorPoolPerBracketType": false,
  "files.eol": "\n",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "extensions.ignoreRecommendations": false,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": true,
  "editor.linkedEditing": true,
  "editor.suggestSelection": "first",
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
    "source.sortPackageJson":true
  },
  "files.associations": { "*.tsx": "typescriptreact" },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 3600000,
  "explorer.confirmDelete": false,
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.activeIconPack": "react_redux",
  "indentRainbow.colors": ["rgba(34, 34, 34, 1)", "rgba(17, 17, 17, 1)"],
  "indentRainbow.ignoreErrorLanguages": ["python", "docker"],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "[json][graphql][typescript][javascript][javascriptreact][typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.importModuleSpecifier": "relative",
  "isort.args": ["-rc", "--atomic"],
  "material-icon-theme.files.associations": {
    "Dockerfile.*": "docker",
    "docker-compose.*": "docker",
    ".env.*": "tune"
  },
  "material-icon-theme.folders.associations": {
    "aws-utils": "aws",
    "aws-paths": "aws",
    "blockchain": "contract",
    "hive": "contract",
    "solidity": "contract",
    "discord": "core",
    "chrome_extension": "middleware"
  },
  "workbench.editor.wrapTabs": true,
  "go.toolsManagement.autoUpdate": true,
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "workbench.startupEditor": "none",
  "explorer.confirmDragAndDrop": false,
  "git.autofetch": true
}
